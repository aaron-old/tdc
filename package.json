{
  "name": "tdoggs-corner",
  "version": "1.0.0",
  "engines": {
    "node": "<=6.11.1"
  },
  "description": "A boxing blog.",
  "scripts": {
    "start": "concurrently \"npm run dev-server\" \"npm run client\"",
    "dev-server": "nodemon server/index.js",
    "server": "babel-node server/index.js",
    "client": "babel-node start-client.js",
    "test": "mocha -w \"server/**/*.spec.js\"",
    "heroku-postbuild": "node_modules/.bin/sequelize db:seed:all && cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "proxy": "//localhost:3001/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mustardmandevelopments/tdc.git"
  },
  "author": "Aaron Young",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mustardmandevelopments/tdc/issues"
  },
  "homepage": "https://github.com/mustardmandevelopments/tdc#readme",
  "devDependencies": {
    "jquery": "^3.2.1",
    "mailgun-js": "^0.12.1",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.8.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "auth0-js": "^8.8.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "compression": "^1.6.2",
    "concurrently": "^3.4.0",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.9",
    "mocha": "^3.5.3",
    "morgan": "^1.8.2",
    "mysql": "^2.13.0",
    "mysql2": "^1.3.5",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "sequelize": "^4.0.0",
    "sequelize-cli": "^2.7.0",
    "sinon": "^4.1.2"
  }
}
